<div class="row-fluid">
	<div class="span12">
		<h3><span class="badge badge-warning">Non Mititic samples</span></h3>
	</div>
</div>
<div class="row-fluid">
	<div class="span12 traintable">
		<% (0..35).each do |row_index| %>
			<div class="row-fluid">
				<ul class="thumbnails">
					<% (0..5).each do |col_index| %>
					<li class="span2">
						<a href="#" class="thumbnail">
							<% code = @detection.negative_training.keys[row_index * 6 + col_index] %>
							<%= image_tag(@detection.negative_training[code][1], alt: code, title: code) %>
						</a>
					</li>
					<% end %>
				</ul>
			</div>
		<% end %>
	</div>
</div>
