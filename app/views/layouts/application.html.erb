<!DOCTYPE html>
<html>
	<head>
		<title><%= full_title(yield(:title)) %></title>
		<%= stylesheet_link_tag    "application", media: "all" %>
		<%= javascript_include_tag(:application) %>
		<%= csrf_meta_tags %>
		<%= render 'layouts/shim' %>
	</head>

	<body>
		<%= render 'layouts/header' %>
		<div class="container-fluid">
			<div class="row-fluid">
				<div class="span2">
					&nbsp;
				</div>
				<div class="span10">
					&nbsp;
				</div>
			</div>
			<div class="row-fluid">
				<div class="span12">
					<%= yield %>
				</div>
			</div>
			<%= debug(params) if Rails.env.development? %>
		</div>
	</body>
</html>
